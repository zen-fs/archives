		"format:check": "prettier --check .",
		"lint": "eslint src",
		"test": "npx zenfs-test -abcf",
		"build": "tsc -p tsconfig.json",
		"build:docs": "typedoc",
		"prepublishOnly": "npm run build"
	},
	"devDependencies": {
		"@types/node": "^22.10.1",
		"c8": "^10.1.3",
		"eslint": "^9.12.0",
		"globals": "^16.0.0",
		"prettier": "^3.2.5",
		"tsx": "^4.19.1",
		"typedoc": "^0.28.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.8.1"
	},
	"peerDependencies": {
		"@zenfs/core": "^2.1.0",
		"kerium": "^1.3.4",
		"memium": "^0.2.0",
		"utilium": "^2.0.0"
	},
	"dependencies": {
		"fflate": "^0.8.2"
	}
