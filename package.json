{
	"name": "@zenfs/iso",
	"version": "0.3.0",
	"description": "Iso backend for ZenFS",
	"main": "dist/index.js",
	"types": "src/index.ts",
	"keywords": [
		"filesystem",
		"node",
		"storage"
	],
	"type": "module",
	"homepage": "https://github.com/zen-fs/iso",
	"author": "James P. <jp@drvortex.dev>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zen-fs/iso.git"
	},
	"bugs": {
		"url": "https://github.com/zen-fs/iso/issues"
	},
	"engines": {
		"node": ">= 18"
	},
	"files": [
		"dist",
		"src",
		"license.md",
		"tsconfig.json"
	],
	"exports": {
		".": "./dist/index.js",
		"./*": "./dist/*"
	},
	"scripts": {
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "eslint src",
		"build": "npx build --package=@zenfs/core --globalName ZenFS_ISO --entry src/index.ts",
		"build:docs": "typedoc --out docs --name 'ZenFS ISO' src/index.ts",
		"test": "echo No tests yet",
		"prepublishOnly": "npm run build"
	},
	"devDependencies": {
		"@eslint/js": "^9.11.1",
		"@fal-works/esbuild-plugin-global-externals": "^2.1.2",
		"@stylistic/eslint-plugin": "^2.8.0",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^20.12.7",
		"esbuild": "^0.24.0",
		"eslint": "^9.11.1",
		"prettier": "^3.2.5",
		"typedoc": "^0.26.7",
		"typescript": "^5.5.0",
		"typescript-eslint": "^8.8.0"
	},
	"peerDependencies": {
		"@zenfs/core": "^1.0.2"
	},
	"dependencies": {
		"utilium": "^0.7.0"
	}
}
